@model IEnumerable<TBContatos.Models.Contato>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.nome)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.formacao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.telefone)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.datacontato)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.melhorhorario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.primeirocontato)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.numerocontatos)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.dataprimeirocontato)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.dataultimocontato)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.dataproximocontato)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.obs)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.nome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.formacao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.telefone)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.datacontato)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.melhorhorario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.primeirocontato)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.numerocontatos)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.dataprimeirocontato)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.dataultimocontato)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.dataproximocontato)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.obs)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
